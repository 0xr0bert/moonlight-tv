    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug over SSH",
            "type": "gdb",
            "request": "launch",
            "target": "moonlight",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "pathSubstitutions": {
                "${workspaceFolder}": "/media/developer/apps/usr/palm/applications/com.limelight.webos",
                "/opt/webos-sdk-x86_64/1.0.g/sysroots/armv7a-neon-webos-linux-gnueabi/usr/src/debug/glibc": "/usr/src/debug/lib32-glibc"
            },
            "ssh": {
                "host": "192.168.4.104",
                "port": 9922,
                "forwardX11": false,
                "cwd": "/media/developer/apps/usr/palm/applications/com.limelight.webos",
                "keyfile": "/home/mariotaku/.ssh/hometv_webos_unlocked",
                "user": "prisoner"
            }
        },
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to gdbserver",
            "executable": "${workspaceRoot}/build/moonlight",
            "target": "192.168.4.104:9527",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "gdb-multiarch",
            "debugger_args": [
                "-ex",
                "set architecture armv7"
            ],
            "valuesFormatting": "parseText",
        }
    ]
}